<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Aline Denizot{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Inconsolata&family=Montserrat&display=swap" rel="stylesheet">
    <script type='text/javascript'>
        var text = 'DÃ©veloppeuse Web - Fullstack';
        var montimer;
        var cmpt = 0;
        function typewriter(){
            link = document.getElementById('link');
            if(cmpt < text.length){
                courant = link.innerHTML.substring(0, link.innerHTML.length -1);
                courant += text.charAt(cmpt)+" ";
                link.innerHTML = courant;
            }
            else{
                link.innerHTML = "";
                cmpt = -1;
            }
            cmpt++;
            setTimeout("typewriter()",100);
        }
        window.onload = function(){
            typewriter();
        }
    </script>
</head>
<body>
<header>
    {% include "_navbar.html.twig" %}
</header>

{% block body %}{% endblock %}

<footer>
    {% include "_footer.html.twig" %}
</footer>

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
